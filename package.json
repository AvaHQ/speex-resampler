{
  "name": "speex-resampler",
  "version": "1.0.11",
  "description": "An implementation of Speex audio resampler on NodeJS with N-API",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "cmake-js compile",
    "build:debug": "cmake-js compile -D",
    "deploy": "np --no-tests",
    "test": "node src/test.js",
    "install": "prebuild-install -r napi --target 3 || npm run build",
    "build-binaries": "prebuild -r napi -t 3 --backend cmake-js --verbose",
    "build-and-upload": "run-script-os",
    "build-and-upload:default": "prebuild -r napi -t 4 --backend cmake-js --verbose -u $GITHUB_TOKEN",
    "build-and-upload:win32": "prebuild -r napi -t 4 --backend cmake-js --verbose -u %GITHUB_TOKEN%"
  },
  "author": "Guillaume Besson <guillaume@besson.co>",
  "repository": "https://github.com/geekuillaume/node-speex-resampler",
  "keywords": [
    "audio",
    "resampling",
    "pcm",
    "speex"
  ],
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "cmake-js": "^6.0.0",
    "node-addon-api": "^2.0.0",
    "np": "^6.2.1",
    "prebuild": "^10.0.0"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  }
}
