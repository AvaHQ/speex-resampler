{
  "name": "speex-resampler",
  "version": "1.0.9",
  "description": "An implementation of Speex audio resampler on NodeJS with N-API",
  "main": "src/index.js",
  "types": "src/index.d.ts",
  "scripts": {
    "build": "cmake-js compile",
    "test": "node src/test.js",
    "build:debug": "cmake-js compile -D",
    "build:release": "prebuild -t 3 -r napi --backend cmake-js",
    "build:all-releases": "bash scripts/cross-build.sh",
    "upload-all": "prebuild --upload-all $GITHUB_TOKEN",
    "install": "prebuild-install -r napi || npm run build"
  },
  "author": "Guillaume Besson <guillaume@besson.co>",
  "repository": "https://github.com/geekuillaume/node-speex-resampler",
  "keywords": [
    "audio",
    "resampling",
    "pcm",
    "speex"
  ],
  "license": "MIT",
  "dependencies": {
    "bindings": "^1.5.0",
    "prebuild-install": "^5.3.3"
  },
  "devDependencies": {
    "cmake-js": "^6.0.0",
    "node-addon-api": "^2.0.0",
    "prebuild": "^10.0.0"
  },
  "binary": {
    "napi_versions": [
      3
    ]
  }
}
